<!DOCTYPE html>
<html>
  <head>
    <title>TaskManagement</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%if current_user%>
      <div class="align_right"><%= link_to "Log out", destroy_user_session_path, :method => :delete %></div>
    <%end%>
    <%= yield %>
  </body>
</html>
